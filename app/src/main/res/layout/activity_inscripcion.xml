<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AgregarEvento.Inscripcion">

    <FrameLayout
        android:id="@+id/BarraSuperior"
        android:layout_width="match_parent"
        android:layout_height="77dp"
        android:background="@drawable/custom_background"
        android:elevation="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="I.E.S"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="65sp"
            tools:ignore="HardcodedText,VisualLintOverlap"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

    </FrameLayout>



    <EditText
        android:id="@+id/nombreEstudiante"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:background="@drawable/boton_redondo"
        android:ems="10"
        android:inputType="text"
        android:text="Nombre"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.150"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/dniEstudiante"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:background="@drawable/boton_redondo"
        android:ems="10"
        android:inputType="text"
        android:text="D.N.I"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.350"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/apellidoEstudiante"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:background="@drawable/boton_redondo"
        android:ems="10"
        android:inputType="text"
        android:text="Apellido"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.250"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/correoEstudiante"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:background="@drawable/boton_redondo"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="Correo Electronico"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.550"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/celularEstudiante"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:background="@drawable/boton_redondo"
        android:ems="10"
        android:inputType="number"
        android:text="Celular"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.450"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/enviar"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:background="@drawable/boton_redondo_rojo"
        android:text="Enviar inscripcion"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.700" />

    <TextView
        android:id="@+id/cancelar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancelar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.800"
        android:clickable="true"
        android:contextClickable="true"
        android:textColor="@color/black"
        android:focusable="true" />


</androidx.constraintlayout.widget.ConstraintLayout>